{
  "ApplicationName": "SmartHub",
  "Use_InMemoryDb": false,
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "sqlConnection": "Server=localhost;Port=5432;User ID=<name>;Password=<pwd>;Database=SmartHubDatabase;Integrated Security=true;"
  },
  "JwtSettings": {
    "Key": "<key>",
    "Issuer": "SmartHub",
    "Audience": "SmartHubUser",
    "LifeTimeInMinutes": 60
  },
  "HostOptions": {
    "ShutdownTimeout": "00:00:20"
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  }
}
